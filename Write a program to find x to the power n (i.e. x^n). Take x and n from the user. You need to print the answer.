import java.util.*;

public class Power {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter the base number:");
        int x = sc.nextInt();
        System.out.println("Enter the exponent:");
        int n = sc.nextInt();
        int result = 1;
        while (n!= 0) {
            result *= x;
            --n;
        }
        System.out.println("Result: " +result);
    }
}
